<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jetm="http://jetm.void.fm/schema/jetm_spring_config_1_2"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
                           http://jetm.void.fm/schema/jetm_spring_config_1_2 http://jetm.void.fm/schema/jetm_spring_config_1_2.xsd">


  <jetm:runtime type="nested" timer="default">
    <jetm:features>
      <jetm:interval-buffer interval="5000"/>
      <jetm:persistence />
      <jetm:raw-data-log type="log4j" category="blabla"/>
    </jetm:features>
  </jetm:runtime>

</beans>